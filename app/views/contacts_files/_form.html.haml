= s3_uploader_form callback_url: contacts_files_url, 
                  callback_param: :file_url, 
                  id: 's3-uploader',
                  max_file_size: 3.megabytes do
  
  = file_field_tag :file, multiple: true

= render 'shared/upload_progress_bar'

:javascript
  $('#s3-uploader').S3Uploader()